<?xml version="1.0" encoding="UTF-8"?>
<AcfConfigurationDocument
  xmlns="http://www.phoenixcontact.com/schema/acfconfig"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.phoenixcontact.com/schema/acfconfig.xsd"
  schemaVersion="1.0" >

  <Libraries>
    <Library name="$(namespace).$(name)Library" binaryPath="./lib/lib$(name).so" />
  </Libraries>

  <Components>
    <Component name="$(component)1" type="$(namespace).$(component)" library="$(namespace).$(root.name.format.lastNamespacePart.format.escapeProjectName)Library" />
  </Components>

</AcfConfigurationDocument>