<?xml version="1.0" encoding="UTF-8"?>
<AcfConfigurationDocument
  xmlns="http://www.phoenixcontact.com/schema/acfconfig"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.phoenixcontact.com/schema/acfconfig.xsd"
  schemaVersion="1.0" >

  <Processes>
    <Process name="$(name)Process" settingsPath="$ARP_ACF_SETTINGS_FILE$" />
  </Processes>

  <Libraries>
    <Library name="$(namespace).$(name)Library" binaryPath="./lib/lib$(namespace).so" />
  </Libraries>

  <Components>
    <Component name="$(component)1" type="$(root.namespace.format.cppFullName)::$(component)" library="$(namespace).$(root.name.format.lastNamespacePart.format.escapeProjectName)Library" />
  </Components>

</AcfConfigurationDocument>