<?xml version="1.0" encoding="utf-8"?>
<MetaConfigurationDocument xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" schemaVersion="2.0" xmlns="http://www.phoenixcontact.com/schema/metaconfig">
  <Program type="$(name)">
    <Ports>
$([foreach]port[in]ports)
      <Port name="$(port.name.format.escapeXmlNames)" type="$(port.format.typeMetaDataFormat.format.convertAngleToSquare)" attributes="$(port.attributes.format.metaAttributes)" dimensions="$(port.multiplicity.format.dimensions)" />
$([end-foreach])
    </Ports>
  </Program>
</MetaConfigurationDocument>