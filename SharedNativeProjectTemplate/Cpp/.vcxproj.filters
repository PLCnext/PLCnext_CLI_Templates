<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ItemGroup Condition="Exists('src/$(name)-version.h')">
    <ClCompile Include="intermediate/code/$(name)-cli32.cpp">
      <Filter>intermediate</Filter>
    </ClCompile>
    <ClCompile Include="intermediate/code/$(name)-cli64.cpp">
      <Filter>intermediate</Filter>
    </ClCompile>
    <ClCompile Include="src/$(name).cpp">
      <Filter>src</Filter>
    </ClCompile>
    <ClInclude Include="src/$(name)-cli32.h">
      <Filter>src</Filter>
    </ClInclude>
    <ClInclude Include="src/$(name)-cli64.h">
      <Filter>src</Filter>
    </ClInclude>
    <ClInclude Include="src/$(name)-version.h">
      <Filter>src</Filter>
    </ClInclude>
  </ItemGroup>
  <ItemGroup>
    <ClInclude Include="src/$(name).h">
      <Filter>src</Filter>
    </ClInclude>
  </ItemGroup>
  <ItemGroup>
    <None Include="CMakeLists.txt" />
    <None Include="EclrNative.cmake" />
  </ItemGroup>
  <ItemGroup>
    <Filter Include="intermediate">
      <UniqueIdentifier>$(guid)</UniqueIdentifier>
    </Filter>
    <Filter Include="src">
      <UniqueIdentifier>$(guid)</UniqueIdentifier>
    </Filter>
  </ItemGroup>
</Project>